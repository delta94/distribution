{
  "name": "distribution",
  "version": "1.0.0",
  "description": "react-redux-router-typescript",
  "egg": {
    "typescript": true
  },
  "scripts": {
    "start": "EGG_SERVER_ENV=prod beidou start",
    "test-start": "EGG_SERVER_ENV=local beidou start",
    "debug": "beidou debug",
    "stop": "beidou stop",
    "dev": "beidou dev",
    "build:server": "ets && tsc -p tsconfig-server.json",
    "build:client": "beidou build",
    "build": "EGG_SERVER_ENV=prod ets clean && npm run build:client && npm run build:server",
    "clean": "ets clean",
    "test-jenkins": "mocha --reporter mocha-junit-reporter --reporter-options mochaFile=./reports/test-results.xml"
  },
  "dependencies": {
    "@babel/core": "7.0.0-beta.51",
    "@babel/plugin-proposal-class-properties": "7.0.0-beta.51",
    "@babel/plugin-proposal-object-rest-spread": "7.0.0-beta.51",
    "@babel/plugin-transform-modules-commonjs": "7.0.0-beta.51",
    "@babel/plugin-transform-runtime": "7.0.0-beta.51",
    "@babel/plugin-transform-typescript": "7.0.0-beta.51",
    "@babel/preset-env": "7.0.0-beta.51",
    "@babel/preset-react": "7.0.0-beta.51",
    "@babel/preset-stage-2": "7.0.0-beta.51",
    "@babel/preset-typescript": "7.0.0-beta.51",
    "@babel/register": "7.0.0-beta.51",
    "@babel/runtime": "7.0.0-beta.51",
    "@types/node": "^10.7.1",
    "axios": "^0.18.0",
    "babel-loader": "8.0.0-beta.4",
    "babel-plugin-dynamic-import-node": "^2.0.0",
    "babel-plugin-import-inspector": "^2.0.0",
    "babel-polyfill": "^6.26.0",
    "beidou-cli": "^1.0.10",
    "beidou-core": "^1.0.10",
    "egg": "^2.10.0",
    "egg-ts-helper": "^1.9.2",
    "js-cookie": "^2.2.0",
    "rc-form": "^2.4.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-hot-loader": "^4.3.3",
    "react-loadable": "^5.5.0",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.3.1",
    "react-transition-group": "^2.5.3",
    "redux": "^4.0.0",
    "redux-actions": "^2.6.1",
    "redux-saga": "^0.16.0",
    "tslib": "^1.9.3"
  },
  "devDependencies": {
    "@types/echarts": "^4.1.3",
    "@types/fullpage.js": "^2.9.0",
    "@types/react": "^16.4.6",
    "@types/react-router": "^4.4.3",
    "antd-mobile": "^2.2.8",
    "babel-plugin-import": "^1.11.0",
    "egg-proxy": "^1.1.0",
    "mocha": "^5.2.0",
    "mocha-junit-reporter": "^1.18.0",
    "postcss-pxtorem": "^4.0.1",
    "tslint": "^5.12.1",
    "typescript": "^3.2.4",
    "webpack": "^3.12.0"
  },
  "engines": {
    "node": ">= 8.0.0"
  },
  "license": "MIT"
}
